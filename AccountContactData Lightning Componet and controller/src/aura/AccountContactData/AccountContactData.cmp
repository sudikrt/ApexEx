<aura:component controller="AccountContactHandler" implements="flexipage:availableForAllPageTypes,force:hasRecordId" access="global" >
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="errMsg" type="String" />
    <aura:attribute name="contactList" type="List" />
    
    <aura:handler name="init"  value="{!this}"  action="{!c.doInit}"/>
    
    <div>
        <aura:if isTrue="{! v.contactList.length > 0}">
            <aura:iteration items="{! v.contactList}" var="item">
                {! item.Name} 
            </aura:iteration>
        </aura:if>
        {!v.errrMsg}
    </div>
</aura:component>